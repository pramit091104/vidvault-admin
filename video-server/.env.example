# Google Cloud Storage Configuration
GCS_PROJECT_ID=your_project_id
GCS_BUCKET_NAME=your_bucket_name
GCS_CREDENTIALS_BASE64=your_base64_encoded_service_account_json
AUTO_CONFIGURE_GCS_CORS=true
GCS_CORS_ORIGINS=https://yourdomain.com,http://localhost:5173,http://localhost:8080

# Razorpay Configuration (Backend - KEEP SECRET)
RAZORPAY_KEY_ID=rzp_test_or_live_key
RAZORPAY_KEY_SECRET=your_razorpay_secret_key
RAZORPAY_WEBHOOK_SECRET=your_webhook_secret

# Email Notification Configuration
GMAIL_USER=your_email@gmail.com
GMAIL_APP_PASSWORD=your_gmail_app_password

# Redis Configuration (Optional)
REDIS_URL=redis://localhost:6379

# Server Configuration
PORT=3001
ALLOWED_ORIGINS=http://localhost:5173,http://localhost:3000,https://yourdomain.com
