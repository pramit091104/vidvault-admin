{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "api/razorpay/create-order.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/razorpay/verify-payment.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/signed-url.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/test.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/api/razorpay/create-order",
      "dest": "/api/razorpay/create-order.js"
    },
    {
      "src": "/api/razorpay/verify-payment",
      "dest": "/api/razorpay/verify-payment.js"
    },
    {
      "src": "/api/signed-url",
      "dest": "/api/signed-url.js"
    },
    {
      "src": "/api/test",
      "dest": "/api/test.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}