{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "api/subscription.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/clients.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/upload.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/storage.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/payment.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/test.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/api/subscription/(.*)",
      "dest": "/api/subscription.js"
    },
    {
      "src": "/api/clients/(.*)",
      "dest": "/api/clients.js"
    },
    {
      "src": "/api/upload/(.*)",
      "dest": "/api/upload.js"
    },
    {
      "src": "/api/storage/(.*)",
      "dest": "/api/storage.js"
    },
    {
      "src": "/api/payment/(.*)",
      "dest": "/api/payment.js"
    },
    {
      "src": "/api/test",
      "dest": "/api/test.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}